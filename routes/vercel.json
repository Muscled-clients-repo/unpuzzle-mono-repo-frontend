{
  "rewrites": [
    {
      "source": "/instructor/:match*",
      "destination": "https://instructor.nazmulcodes.org/:match*"
    },
    {
      "source": "/sign-up",
      "destination": "https://dev.nazmulcodes.org/sign-up"
    },
    {
      "source": "/sign-in",
      "destination": "https://dev.nazmulcodes.org/sign-in"
    },
    {
      "source": "/api/:match*",
      "destination": "https://dev.nazmulcodes.org/api/:match*"
    },
    {
      "source": "/:match*",
      "destination": "https://student.nazmulcodes.org/courses/:match*"
    }
  ]
}
