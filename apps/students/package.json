{
  "name": "unpuzzle-ai-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p ${PORT:-3001}",
    "build": "next build",
    "build:no-lint": "SKIP_ENV_VALIDATION=1 next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"app/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"app/**/*.{ts,tsx,js,jsx,json,css}\"",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "typecheck": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "optimize:check": "node scripts/optimize-check.js",
    "optimize:types": "tsc --noEmit -p tsconfig.strict.json",
    "optimize:unused": "npx depcheck",
    "optimize:bundle": "npm run analyze",
    "lighthouse": "npx lighthouse http://localhost:3000 --view",
    "perf": "npm run optimize:check"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@reduxjs/toolkit": "^2.8.2",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.4.0",
    "@types/socket.io-client": "^1.4.36",
    "@wavesurfer/react": "^1.0.11",
    "axios": "^1.10.0",
    "chart.js": "^4.4.9",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "get-blob-duration": "^1.2.0",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-draggable": "^4.4.6",
    "react-icons": "^5.5.0",
    "react-media-recorder": "^1.6.5",
    "react-redux": "^9.2.0",
    "react-toastify": "^11.0.5",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "video-editing-timeline-react": "^1.0.0",
    "wavesurfer.js": "^7.9.9"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.4",
    "@tailwindcss/postcss": "^4.1.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "autoprefixer": "^10.4.20",
    "eslint-config-next": "^15.4.4",
    "eslint-config-prettier": "^10.1.8",
    "glob": "^11.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5"
  }
}
