{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/apps/instructor/:path*",
      "destination": "https://unpuzzle-instructor.vercel.app/:path*"
    },
    {
      "source": "/apps/student/:path*", 
      "destination": "https://unpuzzle-student.vercel.app/:path*"
    }
  ]
}