{
  "buildCommand": "pnpm turbo build",
  "ignoreCommand": "npx turbo-ignore",
  "framework": null,
  "rewrites": [
    {
      "source": "/instructor/:path*",
      "destination": "/instructor/:path*"
    },
    {
      "source": "/student/:path*", 
      "destination": "/student/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/:path*"
    }
  ]
}